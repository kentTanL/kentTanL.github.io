---  
layout: none  
--- 

<?xml version="1.0" encoding="utf-8"?>
<urlset>
    {% for post in site.posts limit:100 %}  
        <url>
            <loc>{{ site.baseurl}}{{ site.baseurl }}{{ post.url }}</loc>
            <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
        </url>
    {% endfor %} 
</urlset>
